<p id="notice"><%= notice %></p>

<h1>Listing Scanneds</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scanneds.each do |scanned| %>
      <tr>
        <td><%= link_to 'Show', scanned %></td>
        <td><%= link_to 'Edit', edit_scanned_path(scanned) %></td>
        <td><%= link_to 'Destroy', scanned, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scanned', new_scanned_path %>
